{
    "plugins":["jsdoc-http-plugin"],
    "source":{
        "include":["src"],
        "includePattern": ".js$",
        "exclude":"(node_modules|docs)"
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts":{
        "recurse": true, 
        "destination":"./docs/",
        "template": "node_modules/clean-jsdoc-theme",
        "theme_opts": {
            "theme": "dark"
        },
        "readme":"./README.md"
    }
}